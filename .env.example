# FastAPI / AlertManager configuration
APP_NAME=Omnis Partner API
APP_VERSION=3.3.12.10-SNAPSHOT

# Feature switches (true/false)
OMNIS_SWITCH_WEBSQL=false
OMNIS_SWITCH_ALERTMANAGER=true
OMNIS_SWITCH_ALERTRECOVERY=false
OMNIS_SWITCH_DEPLOY=false
OMNIS_SWITCH_ESBAGENT=false
OMNIS_SWITCH_FINEREPORT=false
OMNIS_SWITCH_CBHREPORT=false

# Shared MySQL connection
DB_HOST=127.0.0.1
DB_PORT=3306
DB_NAME=omnis
DB_USER=omnis
DB_PASSWORD=change_me
DB_CHARSET=utf8mb4
# Alternatively, specify DATABASE_URL=mysql+pymysql://user:pass@host:port/dbname?charset=utf8mb4

# Nexus repository
NEXUS_BASE_URL=http://localhost:8081
NEXUS_REPOSITORY=maven-releases
NEXUS_USERNAME=
NEXUS_PASSWORD=
NEXUS_DOWNLOAD_DIR=new/tmp/nexus
